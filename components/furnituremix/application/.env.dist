# MAILER_DSN = "sendgrid://${APIKEY}"

HTTP_CACHE_SERVICE=fastly
JWT_SECRET=xXx

# If you want to enabled Fake Payment Methods: (separated by comma)
# Available: coin, card
CRYSTAL_PAYMENTS="coin,card"


# You can override the hostname used when running a superfast store by setting
# SUPERFAST_HOST. Use this to use the superfast adapter without running caddy
# SUPERFAST_HOST=my-poc.superfast.store
