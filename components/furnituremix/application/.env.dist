# Do not put confidential information in this file. cp .env.dist .env

PORT=3018

SUPERFAST_TENANT_IDENTIFIER=superfast
SUPERFAST_ACCESS_TOKEN_ID=do_not_put_confidential_data_here
SUPERFAST_ACCESS_TOKEN_SECRET=do_not_put_confidential_data_here
ENCRYPTED_PARAMS_SECRET=xXx

# STORAGE_DSN="memory://"
STORAGE_DSN="redis://localhost:3023"
MAILER_DSN="smtp://localhost:3021/?pool=true"
